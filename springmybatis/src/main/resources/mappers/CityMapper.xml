<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.yang.dao.mapper.CityMapper">
    <select id="selectAll" resultType="com.yang.dao.entry.City">
        select * from t_city
    </select>
    <select id="selectById" resultType="com.yang.dao.entry.City">
        select * from t_city
        <where>
            <if test="id!=null">
                id=#{id}
            </if>
        </where>
    </select>
</mapper>